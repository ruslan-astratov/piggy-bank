{
  "scripts": {
    "serve": "parcel index.html",
    "start": "parcel ./*.html",
    "clean": "rm -rf dist",
    "build": "parcel build ./*.html --public-url https://ruslan-astratov.github.io/piggy-bank/",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "gh-pages": "^3.1.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "parcel": "*"
  }
}
